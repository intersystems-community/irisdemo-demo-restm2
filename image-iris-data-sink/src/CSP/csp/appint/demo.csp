<!DOCTYPE html>

<html>
	<head>

		<meta charset="UTF-8">
		<meta http-equiv="cache-control" content="no-cache" />
		<meta http-equiv="expires" content="0" />
		<meta http-equiv="pragma" content="no-cache" />

		<title>InterSystems IRIS Multi-Model with Schema Migration Demo</title>

		<script language="javascript">

		var ip = window.location.hostname;

		function openWindows()
		{
			var time=1000;
			var callNumber=0
			var timeout = function () { return ++callNumber*time };

				/*
				A timeout between each opened tab is necessary for Safari.
				Otherwise, Safari will simply open all windows on a single tab.
				With the timeout, Safari will first block the popups. Once the user tells
				safari that it should allow popups, the user should be able to try opening all
				windows again and all the tabs will be opened, one every second, on a new tab.
				*/
				
				setTimeout("openWorkflow()", timeout());
				
				setTimeout("openDataStore()", timeout());
				
				setTimeout("openMessageTrace()", timeout());

				setTimeout("openSimulator()", timeout());
								
				setTimeout("openDataLookup()", timeout());
				
				setTimeout("openDataTransformationV2()", timeout());
				
				setTimeout("openDataTransformationV1()", timeout());
				
				setTimeout("openBusinessProcess()", timeout());

				setTimeout("openProduction()", timeout());
			}

			function window_open(url)
			{
				var winReference = window.open();
				winReference.location = url;
				winReference.parent.focus();
			}

			function openSimulator()
			{
				window_open('http://' + ip + ':10000');
			}

			function openProduction()
			{
				window_open('EnsPortal.ProductionConfig.zen?$NAMESPACE=APPINT&IRISUserName=SuperUser&IRISPassword=sys');
			}

			function openBusinessProcess()
			{
				window_open('EnsPortal.BPLEditor.zen?NAMESPACE=APPINT&BP=IRISDemo.BP.Customer.Process.bpl&IRISUserName=SuperUser&IRISPassword=sys')
			}

			function openDataTransformationV1()
			{
				window_open('EnsPortal.DTLEditor.zen?NAMESPACE=APPINT&DT=Original.V1.DTL.Customer.dtl&IRISUserName=SuperUser&IRISPassword=sys')
			}

			function openDataTransformationV2()
			{
				window_open('EnsPortal.DTLEditor.zen?NAMESPACE=APPINT&DT=Original.V2.DTL.Customer.dtl&IRISUserName=SuperUser&IRISPassword=sys')
			}

			function openDataLookup()
			{
				window_open('EnsPortal.LookupSettings.zen?LookupTable=US_States_Code_To_Desc.lut&NAMESPACE=APPINT&IRISUserName=SuperUser&IRISPassword=sys')
			}

			function openMessageTrace()
			{
				window_open('EnsPortal.MessageViewer.zen?$NAMESPACE=APPINT&IRISUserName=SuperUser&IRISPassword=sys')
			}

			function openWorkflow()
			{
				window_open('_DeepSee.UserPortal.Workflow.zen?$NAMESPACE=APPINT&IRISUserName=SuperUser&IRISPassword=sys')
			}

			function openDataStore()
			{
				window_open('http://' + ip + ':10001/csp/sys/exp/%25CSP.UI.Portal.SQL.Home.zen?$NAMESPACE=APPINT&IRISUserName=SuperUser&IRISPassword=sys')
			}
			
			function openOriginalV1()
			{
				window_open('http://' + ip + ':10001/csp/appint/rest/v1/1')
			}

			function openOriginalV2()
			{
				window_open('http://' + ip + ':10001/csp/appint/rest/v2/1')
			}
				
			function openInstructions()
			{
				var btn = document.getElementById('instructionsbtn');
				var instructions = document.getElementById('instructions');

				if (btn.value == 'Show Instructions')
				{
					btn.value = 'Hide Instructions';
					instructions.hidden=false;
				}
				else
				{
					btn.value = 'Show Instructions';
					instructions.hidden=true;
				}
			}

		</script>
		<style>
		.Button {
			font-family: Arial;
			font-size: 12px;
			font-weight: bold;
			color: darkblue;
			background-color: #FFFFFF;
			width: 200px;
			border-radius: 5px;
		}
		.Button:hover {
			color: #000000;
			background-color: #FFFFFF;
		}
		
		td.leftFormField
		{
			text-align: left; background-color: #eeeeee; padding: 5px;
		}

		
		</style>
	</head>

	<body >

		<!-- Image Map Generated by http://www.image-map.net/ -->
		<center>
			<img src="landing-page.png" usemap="#image-map" id="demoimg">
		</center>

		<map name="image-map">
		    <area target="" alt="Open Data Simulator" title="Open Data Simulator" href="javascript:openSimulator()" coords="3,360,153,701" shape="rect">
		    <area target="" alt="Open Original Document Schema V1" title="Open Original Document Schema V1" href="javascript:openOriginalV1()" coords="491,365,616,515" shape="rect">
		    <area target="" alt="Open Original Document Schema V2" title="Open Original Document Schema V2" href="javascript:openOriginalV2()" coords="491,568,613,722" shape="rect">
		    <area target="" alt="Open Transformation for Schema V1" title="Open Transformation for Schema V1" href="javascript:openDataTransformationV1()" coords="857,334,958,446" shape="rect">
		    <area target="" alt="Open Transformation for Schema V2" title="Open Transformation for Schema V2" href="javascript:openDataTransformationV2()" coords="977,334,1077,448" shape="rect">
		    <area target="" alt="Open Data Lookup (cross-walk to ref. data)" title="Open Data Lookup (cross-walk to ref. data)" href="javascript:openDataLookup()" coords="1099,327,1201,455" shape="rect">
		    <area target="" alt="Open Normalization Process" title="Open Normalization Process" href="javascript:openBusinessProcess()" coords="857,467,1180,631" shape="rect">
		    <area target="" alt="Open Ingestion Service in Production" title="Open Ingestion Service in Production" href="javascript:openProduction()" coords="620,438,810,519" shape="rect">
		    <area target="" alt="Open Ingestion Service in Production" title="Open Ingestion Service in Production" href="javascript:openProduction()" coords="624,582,813,661" shape="rect">
		    <area target="" alt="Open Data Lineage" title="Open Data Lineage" href="javascript:openMessageTrace()" coords="1212,509,1380,593" shape="rect">
		    <area target="" alt="Open Workflow Inbox" title="Open Workflow Inbox" href="javascript:openWorkflow()" coords="1080,733,1255,952" shape="rect">
		    <area target="" alt="Open Canonical Data Store" title="Open Canonical Data Store" href="javascript:openDataStore()" coords="1398,477,1545,635" shape="rect">
		</map>
						
		<table width="100%" style="text-align: center;">
			<tr>
				<td><input ID="BTNALL" type="button" class="Button" value="Open All Windows" onclick="openWindows()"/></td>
			</tr>
		</table>


		<script type="text/javascript" src="imageMapResizer.min.js"></script>

		<script language="javascript">
			var demoimg = document.getElementById('demoimg');

			demoimg.style.width="90%";

			imageMapResize();
		</script>

	</body>
</html>
